@model PEF.Models.Members
@using PEF.Models
@{
    ViewData["Title"] = "حساب باحث عن عمل جديد";
}
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
 <link rel="stylesheet" href="~/fastselect/dist/ar_fastselect.css">
<style>
    #youtube-video{
        position: fixed;
        bottom: 20px;
        left: 20px;
        width: 308px;
    }

    .youtube-container {
        position: relative;
        padding-bottom: 56.25%;
        height: 0;
        overflow: hidden;
    }

    .youtube-container iframe {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }

    @@media(max-width:480px){
        #youtube-video{
            width: 350px;
        }

        .youtube-container iframe{
            width:350px;
        }
    }
</style>

<div class="container">
<div class="row d-flex justify-content-center mt-4 mb-5">
        <div class="text-center mb-3"><h2>تسجيل الباحثين عن فرص تشغيل/ فرص تدريب على رأس العمل </h2></div>
    <div class="col-12 col-md-9 pt-3 form-box">
        <form asp-action="CreateMember" id="MembersForm" method="post" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="row">
            <div class="form-group col-6 col-md-3 mb-3">
                <label for="FirstName" class="control-label">الأسم الأول <span class="required" >*</span></label>
                <input asp-for="FirstName" class="form-control" />
                <span asp-validation-for="FirstName" class="text-danger"></span>
            </div>
            <div class="form-group col-6 col-md-3 mb-3">
                <label for="FatherName" class="control-label">اسم الأب <span class="required" >*</span></label>
                <input asp-for="FatherName" class="form-control" />
                <span asp-validation-for="FatherName" class="text-danger"></span>
            </div>
            <div class="form-group col-6 col-md-3  mb-3">
                <label for="GrandName" class="control-label">اسم الجد <span class="required" >*</span></label>
                <input asp-for="GrandName" class="form-control" />
                <span asp-validation-for="GrandName" class="text-danger"></span>
            </div>
            <div class="form-group col-6 col-md-3 mb-3">
                <label for="FamilyName" class="control-label">اسم العائلة <span class="required" >*</span></label>
                <input asp-for="FamilyName" class="form-control" />
                <span asp-validation-for="FamilyName" class="text-danger"></span>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-6 col-md-3 mb-3">
                <label for="IdNum" class="control-label">رقم الهوية <span class="required" >*</span></label>
                <input asp-for="IdNum" maxlength="9" minlength="9" class="form-control" />
                <label id="IdNumExist-error" class="error" for="IdNum" style="display: none;"></label>
                <span asp-validation-for="IdNum" class="text-danger"></span>
            </div>
            <div class="form-group col-6 col-md-3  mb-3">
                <label for="Gender" class="control-label">الجنس <span class="required" >*</span></label>
                <select name="Gender" id="Gender" class="form-control">
                    <option value="">:: حدد الجنس ::</option>
                    <!option value="1" @(Model!=null && Model.Gender == 0 ? "SELECTED":"") >ذكر</!option>
                    <!option value="2" @(Model!=null && Model.Gender == 2 ? "SELECTED":"") >أنثى</!option>
                </select>
                <span asp-validation-for="Gender" class="text-danger"></span>
            </div>
            <div class="form-group col-6 col-md-3  mb-3">
                <label for="SocialId" class="control-label">الحالة الاجتماعية <span class="required">*</span></label>
                <select asp-for="SocialId" class="form-control">
                    <option value="">:: حدد الحالة الاجتماعية ::</option>
                    @{
                        if (ViewBag.SocialId != null)
                        {
                            foreach (LookupSocialStatus item in ViewBag.SocialId)
                            {
                                <!option value="@item.Id" @(Model!=null && Model.SocialId == item.Id ? "SELECTED":"") >@item.ArName</!option>
                            }
                        }
                    }
                </select>
            </div>
            <div class="form-group col-6 col-md-3  mb-3">
                <label for="BirthDate" class="control-label">تاريخ الميلاد <span class="required" >*</span></label>
                <input type="date" asp-for="BirthDate" class="form-control" />
                <span asp-validation-for="BirthDate" class="text-danger"></span>
            </div>
        </div>
        
        <div class="row">
            <div class="form-group col-6 col-md-3 mb-3">
                <label for="FamilyMembersCount" class="control-label">عدد أفراد الأسرة <span class="required" >*</span></label>
                <input asp-for="FamilyMembersCount" class="form-control" />
                <span asp-validation-for="FamilyMembersCount" class="text-danger"></span>
            </div>  
        </div>
        <div class="row">
            <div class="form-group col-6 col-md-3 mb-3">
                <label for="FatherIdNum" class="control-label">رقم هوية الأب <span class="required" >*</span></label>
                <input asp-for="FatherIdNum" maxlength="9" minlength="9" class="form-control" />
                <span asp-validation-for="FatherIdNum" class="text-danger"></span>
            </div>
            <div class="form-group col-6 col-md-3 mb-3">
                        <label for="MotherIdNum" class="control-label">رقم هوية الأم <span class="required">*</span></label>
                <input asp-for="MotherIdNum" maxlength="9" minlength="9" class="form-control" />
                <span asp-validation-for="MotherIdNum" class="text-danger"></span>
            </div>
            <div class="form-group col-6 col-md-3 mb-3" id="partnerIdCont" style="@(Model!=null && Model.SocialId == 2 ? "display:block;" : "display:none;")" >
