                    <!option value="2" @(Model!=null && Model.Interest?.Contains("2") == true ? "SELECTED":"")>تدريب على راس العمل</!option>
                    <!option value="3" @(Model!=null && Model.Interest?.Contains("3") == true ? "SELECTED":"")>تدريب غير مدفوع</!option>
                    <!option value="4" @(Model!=null && Model.Interest?.Contains("4") == true ? "SELECTED":"")>تمويل مشاريع صغيرة</!option>
                </select>
                
            </div>
        </div>
        <div class="row">
            <div class="form-group col-6  mb-3">
                <label for="HealthStatus" class="control-label">الحالة الصحية <span class="required" >*</span></label>
                <select asp-for="HealthStatus" class="form-control" required="required">
                    <option value="">:: حدد الوضع الصحي ::</option>
                    <!option value="1" @(Model!=null && Model.HealthStatus==1 ? "SELECTED":"")>سليم</!option>
                    <!option value="2" @(Model!=null && Model.HealthStatus==2 ? "SELECTED":"")>مريض</!option>
                    <!option value="3" @(Model!=null && Model.HealthStatus==3 ? "SELECTED":"")>ذوي اعاقة</!option>
                </select>
                <span asp-validation-for="HealthStatus" class="text-danger"></span>
            </div>
        </div>
