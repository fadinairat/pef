                <input type="password" asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
            <div class="form-group col-6 col-md-3  mb-3">
                <label for="ConfirmPassword" class="control-label">تأكيد كلمة المرور <span class="required" >*</span></label>
                <input type="password" asp-for="ConfirmPassword" class="form-control" required="required" />
                <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-12  mb-3">
                <label for="Interests" class="control-label">طبيعة الاهتمام <span class="required" >*</span></label>
                <select class="multipleSelect" multiple name="Interests[]" id="Interests">
